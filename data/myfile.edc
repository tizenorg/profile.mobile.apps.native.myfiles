	group { name: "root_layout";
	   parts {
		part { name: "background";
			type: RECT;
			mouse_events: 0;
			scale: 1;
			description {
				state: "default" 0.0;
				rel1 { relative: 0.0 0.0; }
				rel2 { relative: 1.0 1.0; }
				color: 255 255 255 255;
			}
		}

		part {
			name: "category";
			type: SWALLOW;
			scale: 1;
			repeat_events: 0;
