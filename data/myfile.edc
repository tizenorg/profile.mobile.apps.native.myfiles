        scale: 1;
        description {
           state: "default" 0.0;
           min: 0 DATETIME_TOP_PADDING_HEIGHT;
           align: 0.5 1.0;
           rel1 { to: "padding2"; relative: 0 1; }
        }
     }
  }
}

