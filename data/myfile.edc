collections {

	plugins {
	      plugin {
		 name: "touch_sound";  // Name run by RUN_PLUGIN
		 source: "feedback";	 // External edje plugin name
		 param: "FEEDBACK_TYPE_SOUND FEEDBACK_PATTERN_TAP";

	      }
